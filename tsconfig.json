{
	"compilerOptions": {
		"target": "es2022",
		"moduleResolution": "NodeNext",
		"moduleDetection": "force",
		"module": "ES2022",
		"resolveJsonModule": true,
		"allowJs": true,
		"noEmit": true,
		"verbatimModuleSyntax": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"noUncheckedIndexedAccess": true,
		"skipLibCheck": true
	}
}
