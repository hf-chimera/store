{
	"name": "@hf-chimera/store",
	"version": "0.2.1",
	"description": "Cross-end reactivity API - Core store",
	"license": "MIT",
	"author": "hewston",
	"keywords": [
		"state-management",
		"reactivity",
		"reactive",
		"store",
		"cross-platform",
		"typescript",
		"fullstack",
		"client-server",
		"state-sync"
	],
	"type": "module",
	"types": "./dist/index.d.ts",
	"main": "./dist/index.cjs",
